script/source = "extends Control

func _on_back_pressed():
	Sounds.click()
	get_tree().change_scene_to_file(\"res://scenes/menu/main/menu.tscn\")

func _on_survival_pressed():
	Sounds.click()
	Globals.game_mode = E.GAME_TYPE.SURVIVAL
	Levels.set_current_level(1)
	get_tree().change_scene_to_file(\"res://scenes/game/game.tscn\")

func _on_endless_pressed():
	Sounds.click()
	Globals.game_mode = E.GAME_TYPE.ENDLESS
	Levels.set_endless()
	get_tree().change_scene_to_file(\"res://scenes/game/game.tscn\")

func _on_timed_pressed():
	Sounds.click()
	Globals.game_mode = E.GAME_TYPE.TIMED
	Levels.set_timed()
	get_tree().change_scene_to_file(\"res://scenes/game/game.tscn\")
"

